\begin_layout Section
Probit regression with log loss
\end_layout

\begin_layout Standard
Probit regression with random intercept.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
z & \sim & N\left(\alpha_{ij}+\beta x_{ijt}+\gamma_{t},1\right)\\
y & = & 1_{z_{ijt}\geq0}\\
\\
\alpha_{ij} & \sim & N\left(\alpha_{j},\sigma_{\alpha_{j}}^{2}\right)\\
\alpha_{j} & \sim & N\left(\alpha,\sigma_{\alpha}^{2}\right)\\
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y\mid\alpha,\beta,\gamma,x,c,t & \sim & N\left(\beta x+\sum_{c'\in C}\alpha_{c'}1_{c'=c}+\sum_{t'\in T}\gamma_{t}1_{t'=t},\sigma_{y}^{2}\right)\\
\\
\alpha_{c} & \sim & N\left(\xi_{s\left(c\right)},\sigma_{C}^{2}\right)\\
\xi_{s} & \sim & N\left(0,\sigma_{S}^{2}\right)\\
\\
p\left(T=t\right) & = & \frac{1}{5}\\
p\left(C=c\right) & = & \frac{1}{3113}\\
p\left(x\mid c,t\right) & = & \textrm{Empirical distribution}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\alpha_{j} &  & \textrm{state effect}\\
\text{\ensuremath{\gamma_{t}}} &  & \text{\textrm{time effect}}\\
x_{ijt} &  & \textrm{proportion white}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
z_{ij} & = & \alpha+\beta X_{i}+\sum_{j=1}^{n}\gamma_{i}1_{J_{i}=j}+\epsilon_{i}\\
\gamma_{i} & \sim & N\left(0,\sigma_{j}^{2}\right)
\end{eqnarray*}

\end_inset


\end_layout

